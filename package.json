{
  "name": "nutri-hub",
  "private": true,
  "version": "1.0.2",
  "description": "Gym Software.",
  "author": "NutriHub",
  "license": "MIT",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "types": [
      "electron"
    ]
  },
  "build": {
    "appId": "com.Niraj.software",
    "productName": "NutriHub",
    "win": {
      "target": "nsis",
      "icon": "./public/nutrilogo.jpg"
    },
    "directories": {
      "output": "release"
    }
  },
  "repository": "https://github.com/",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build-publish": "tsc && vite build && electron-builder --win --publish always",
    "build-web": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@alvarosacari/electron-pos-printer": "^1.2.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@lottiefiles/lottie-player": "^2.0.2",
    "@mui/material": "^5.14.1",
    "@mui/styled-engine-sc": "^5.12.0",
    "@sbmdkl/nepali-datepicker-reactjs": "^1.2.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "electron-updater": "^6.1.4",
    "file-saver": "^2.0.5",
    "lottie-react": "^2.4.0",
    "nepali-datepicker-reactjs": "^1.1.5",
    "number-to-words": "^1.2.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.13.0",
    "react-select": "^5.8.0",
    "react-simple-typewriter": "^5.0.1",
    "react-typed": "^2.0.12",
    "recharts": "^2.12.7",
    "vite-tsconfig-paths": "^4.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/number-to-words": "^1.2.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.6",
    "electron": "^24.4.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js"
}
